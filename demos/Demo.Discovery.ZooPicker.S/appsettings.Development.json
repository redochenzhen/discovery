{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Keep.Discovery.*": "Debug"
    }
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5003"
      }
    }
  },


  "Discovery": {
    "ShouldRegister": true,

    "ZooPicker": {
      "ConnectionString": "127.0.0.1:2181/services",
      "SessionTimeout": 3000,
      "ConnectionTimeout": 3000,
      "GroupName": "group1",
      //"Password": "123456",

      "Instance": {
        "ServiceName": "testapi",
        "Port": 5003,
        "BalancePolicy": "RoundRobin", //RoundRobin/Random
        "IpAddress": "localhost",
        "PreferIpAddress": true,

        "Weight": 3,
        "ServiceState": "Up", //Up/Down

        "FailTimeout": 10000,
        "MaxFails": 1,
        //"TryNext": {
        //  "Tries": 0,
        //  "Timeout": 0,
        //  "When": "Error,Timeout,NonIdemponent"
        //}
      }
    }
  }
}
